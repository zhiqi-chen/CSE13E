all: rpg

Player.o: Player.h UNIXBOARD.h Player.c
	gcc -c Player.c

Game.o: Player.h Game.h UNIXBOARD.h Game.c
	gcc -c Game.c

Lab08_main.o: Player.h Game.h UNIXBOARD.h Lab08_main.c
	gcc -c Lab08_main.c

rpg: Player.o Game.o Lab08_main.o
	gcc -Wall Player.o Game.o Lab08_main.o -o rpg

clean:
	rm *.o rpg